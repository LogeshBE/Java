
package constructors;


public class Constructors {
    int m,n;
    Constructors()
    {
        System.out.println("Constructors called");
    }
    
    Constructors(int m)
    {   
        this.m=m;
        System.out.println(m);
//        System.out.println("HI"+a);
    }
    Constructors(int m,int n)
    {   this.m=m;
        this.n=n;
        System.out.println(m);
        System.out.println("hello"+m+n);
    }
      void display()
     {  System.out.println(m+n);
     }
     Constructors(int b,int a,String c)
    {
        System.out.println(a+b+c);
    } 
   
 public static void main(String[] args) {
//       Constructors obj1=new Constructors();
       Constructors obj2=new Constructors(50);
//       Constructors obj3=new Constructors(20,10);
//       Constructors obj4=new Constructors(20,20,"Logu");
//        obj3.display();
       System.out.println(obj2.m);
      
      
 }
    
}
