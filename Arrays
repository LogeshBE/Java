package array;


public class array1 {
      int[][] a=new int[2][2];
      int[][] b=new int[][]{
                              {2,2},{3,5}
                            };
      int c[][]={{1,2},{1,2}};
      int d[][];
      int[][][] x=new int[][][]{
                                  {  {1,2},{1,2}
                                  },
                                  {  {1,2},{1,2}
                                  }
                                };
    
}
