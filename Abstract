
package pkgabstract;
abstract class A
{  
    void fun1()
    {
        System.out.println("Hello,LogesH");
    }
   abstract void fun2();

}


public class Abstract extends A{

    public static void main(String[] args) {
        A obj=new Abstract();
        obj.fun1();
        obj.fun2();
        
    }  
       @Override
        void fun2()
    {
        System.out.println("##WELCOME##");
    }
    
}
