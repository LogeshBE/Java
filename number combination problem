
package numbercombination;

public class Numbercombination {

    public static void main(String[] args) {
        int n=1,i,j;
        for( i=1;i<=5;i++){
            System.out.print(i+" ");
            if(i==5){
                System.out.println();
                System.out.println("----------------");
                n++;
                for( j=1;j<=n;j++) System.out.print(j);
                i=n;
            }
        }
    }
    
}
