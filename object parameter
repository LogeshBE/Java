package objectparameter;

class class1{
    
    public int a=10;
    public String name="Logesh";
}
public class Objectparameter {

        public static void main(String[] args) {
           class1 obj1=new class1();
           Objectparameter obj2=new Objectparameter();
           obj2.fun1(obj1);           
    }

    private void fun1(class1 object) {
       
       String  x=object.name;
        System.out.println("The x value is "+x);
    }
    
}
